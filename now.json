{
    "version": 2,
    "name": "nplist",
    "alias": "nplist",
    "builds": [
        { 
            "src": "api/*.go", 
            "use": "@now/go" 
        },
        { 
            "src": "index.html", 
            "use": "@now/html-minifier" 
        },
        { 
            "src": "*.js", 
            "use": "@now/static" 
        },
        {
            "src": "players.json",
            "use": "@now/static" 
        }
    ],
    "routes": [
        { 
            "src": "/api/list", 
            "dest": "/api/list.go" ,
            "headers":
            {
                "Access-Control-Allow-Origin": "*"
            }
        },
        { 
            "src": "/", 
            "dest": "index.html"
        }
    ]
}